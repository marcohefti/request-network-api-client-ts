#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/vitest@4.0.5_@types+node@24.9.2_jiti@2.6.1_jsdom@27.2.0_lightningcss@1.30.2_msw@2.11.6__f7a6146004e780c757198f37f8f7979a/node_modules/vitest/node_modules:/workspace/node_modules/.pnpm/vitest@4.0.5_@types+node@24.9.2_jiti@2.6.1_jsdom@27.2.0_lightningcss@1.30.2_msw@2.11.6__f7a6146004e780c757198f37f8f7979a/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/vitest@4.0.5_@types+node@24.9.2_jiti@2.6.1_jsdom@27.2.0_lightningcss@1.30.2_msw@2.11.6__f7a6146004e780c757198f37f8f7979a/node_modules/vitest/node_modules:/workspace/node_modules/.pnpm/vitest@4.0.5_@types+node@24.9.2_jiti@2.6.1_jsdom@27.2.0_lightningcss@1.30.2_msw@2.11.6__f7a6146004e780c757198f37f8f7979a/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
