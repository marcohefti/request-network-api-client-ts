#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.42.0_@types+node@24.9.2_@types+react@18.3.26_a_1ba7795239b8ea8397ee631a42bab120/node_modules/vitepress/bin/node_modules:/workspace/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.42.0_@types+node@24.9.2_@types+react@18.3.26_a_1ba7795239b8ea8397ee631a42bab120/node_modules/vitepress/node_modules:/workspace/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.42.0_@types+node@24.9.2_@types+react@18.3.26_a_1ba7795239b8ea8397ee631a42bab120/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.42.0_@types+node@24.9.2_@types+react@18.3.26_a_1ba7795239b8ea8397ee631a42bab120/node_modules/vitepress/bin/node_modules:/workspace/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.42.0_@types+node@24.9.2_@types+react@18.3.26_a_1ba7795239b8ea8397ee631a42bab120/node_modules/vitepress/node_modules:/workspace/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.42.0_@types+node@24.9.2_@types+react@18.3.26_a_1ba7795239b8ea8397ee631a42bab120/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
